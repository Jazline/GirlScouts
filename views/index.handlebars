<div id="content">
  <div class="row">
    <div class="col s5 ">
      <div class="carousel carousel-slider">

        <a class="carousel-item" href="#"><img src="/images/IMG_0355.JPG" style="width:600px;height:400px;"></a>
        <a class="carousel-item" href="#"><img src="/images/IMG_0284 (1).JPG" style="width:600px;height:400px;"></a>
        <a class="carousel-item" href="#"><img src="/images/IMG_1515.JPG" style="width:600px;height:400px;"></a>
        <a class="carousel-item" href="#"><img src="/images/IMG_2090.JPG" style="width:600px;height:400px;"></a>
      </div>

    </div>
    <div class="col s7  headline-box">

      <h2 class="main-headline">It's Cookie Season!</h2>

      <h4 class="main-headline">Booths run until Feb 23</h4>
      <a href="https://www.girlscouts.org/en/cookies/all-about-cookies/Meet-the-Cookies.html" class="main-headline headline-link" >Meet the cookies</a>



    </div>
</div>


    <div class="row">

      <div class="col s4 m4">

        <div class="card large" id="news">
          <div id="news-header">
            <h4>News</h4>
          </div>
        </div>

      </div>


      <div class="col s4 m4">

        <div class="card small" id="small-calendar">
          <iframe src="https://calendar.google.com/calendar/embed?src=scogrils74%40gmail.com&ctz=America%2FChicago"
            style="border: 0" width="400" height="300" frameborder="0" scrolling="no"></iframe>
        </div>


      </div>

      <div class="col s4 m4">

        <div class="card large" id="events">
          <h4 id="events-header">Upcoming Events</h4>
          <div id="list">

            {{#each events}}
            <strong>
              <li class="events-list">{{summary}} {{ moment start.date format="MMMM Do YYYY" }} </li>
            </strong>
            {{/each}}



          </div>
        </div>


      </div>


    </div>



    {{!-- <div class="card large" id="chat"> --}}
    {{!-- <h1>Chat section content</h1>
    <div id="posted-comments"></div>

    <input type="text" id="chatComments" placeholder="create a blog post here">
    <button id="submit-chat" type="submit">post</button> --}}





    {{!-- </div> --}}

    <div class="row">
      <div class="col s12 disqus-embed">
        <div id="disqus_thread"></div>
      </div>
    </div>





  </div>

</div>



