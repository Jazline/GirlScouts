<div class="container">

  <div class="row">
    <div class="welcome">
      <h2>Welcome <span class="member-name"></span></h2>
      <div class="container">
        <h2>New Member Entry</h2>
        <form class="newMember">
          <div class="memberForm">
            <label for="firstName">First Name</label>
            <input type="text" class="fname" id="fName" placeholder="First Name">
          </div>
          <div class="newMember">
            <label for="lastName">Last Name</label>
            <input type="text" class="lname" id="lName" placeholder="Last Name">
          </div>
          <div class="newMember">
            <label for="role">Role</label>

            <p>
              <label>
                <input name="role" type="radio" value="Trooper" checked />
                <span>Trooper</span>
              </label>
            </p>

            <p>
              <label>
                <input name="role" type="radio"  value="Leader"/>
                <span>Leader</span>
              </label>
            </p>


            <p>
              <label>
                <input name="role" type="radio"  value="Parent" />
                <span>Parent</span>
              </label>
            </p>


            {{!-- <input type="checkbox" name="role" value="Trooper"><br>
            <input type="checkbox" name="role" value="Leader"><br>
            <input type="checkbox" name="role" value="Parent"><br> --}}
          </div>
          <div class="newMember">
            <label for="child">Child Name</label>
            <input type="text" class="cname" id="cName" placeholder="Child Name">
          </div>

          <button type="submit" class="logButton">Save Changes</button>

          <a class="navbar-brand" href="/logout">
            Logout
          </a>
      </div>
    </div>
    <div>
      <p> </p>
      <br>
    </div>
